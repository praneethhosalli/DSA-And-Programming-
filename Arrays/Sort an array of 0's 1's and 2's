class Solution:
    def sortZeroOneTwo(self, nums):

        left , mid , high = 0 , 0 , len(nums) -1 

        while mid <= high:
            if nums[mid] == 0:
                nums[left] , nums[mid] = nums[mid] , nums[left]
                left+=1
                mid +=1
            elif nums[mid] == 1:
                mid+=1
            else :
                nums[high] , nums[mid] = nums[mid] , nums[high]
                high-=1 

        
