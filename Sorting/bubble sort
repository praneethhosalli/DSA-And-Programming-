class Solution:
    def bubbleSort(self, nums):
        n = len(nums)
        for i in range(n-1 , -1 , -1):
            maxi = i
            for j in range(i , -1 , -1):
                if nums[j]> nums[maxi]:
                    maxi = j
            nums[i] , nums[maxi] = nums[maxi] , nums[i]
                    
        return nums 
