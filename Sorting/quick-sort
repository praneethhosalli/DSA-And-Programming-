class Solution:

    def randomIndex(self , arr , low , high ):
        piviot = arr[low]
        i = low
        j = high
        while(i<j):
            while(arr[i]<=piviot and i<high):
                i+=1
            while(arr[j]>piviot and j > low):
                j-=1
            if i<j:
                arr[i] , arr[j] = arr[j] ,arr[i]
        arr[low] , arr[j] = arr[j] , arr[low]
        return j

    def quickSortHelper(self , nums , low , high ):
        if low < high :
            piviot = self.randomIndex(nums , low , high)
            self.quickSortHelper(nums , low , piviot-1)
            self.quickSortHelper(nums , piviot+1 , high )


    def quickSort(self, nums):
        n = len(nums)
        self.quickSortHelper(nums , 0 , n-1)
        return nums
