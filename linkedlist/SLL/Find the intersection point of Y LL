class Solution:
    def getIntersectionNode(self, headA, headB):

        if headA is None or headB is None:
            return None 

        temp1 = headA 
        temp2 = headB

        while temp1 != temp2 :
            temp1 = temp1.next 
            temp2 = temp2.next 

            if temp1 == temp2 :
                return temp1 
            
            if temp1 is None :
                temp1 = headB 
            if temp2 is None :
                temp2 = headA 

        return temp1 
