# Definition of doubly linked list:
# class ListNode:
#     def __init__(self, val=0, next=None, prev=None):
#         self.val = val
#         self.next = next
#         self.prev = prev

class Solution:
    def deleteKthElement(self, head, k):

        if (head is None) or (head.next is None and k == 1)  :
            return None 

        if head.next is None and k != 1 :
            return head

        
        curr = head 

        if k == 1 :
            curr = curr.next 
            curr.prev = None 
            return curr 

        for i in range(k-1 ) :
            if curr.next  :
                curr = curr.next 
            else :
                return head 

        if curr.next == None :
            curr.prev.next = None 
            curr.prev = None 
            return head 

        curr.prev.next = curr.next 
        curr.next.prev = curr.prev 
        curr.next = None 
        curr.prev = None 
        
        return head 
            
        

        





