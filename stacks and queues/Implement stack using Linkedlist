class Node:
    def __init__(self, d):
        self.val = d
        self.next = None

class LinkedListStack:
    def __init__(self):

        self.head = None 
        self.size  = 0 


    def push(self, x):

        newnode = Node(x)
        newnode.next = self.head 

        self.head = newnode
        self.size +=1 

    def pop(self):

        if self.head is None :
            return -1 

        ans = self.head.val 
        self.head = self.head.next 

        self.size -=1 
        return ans 


    def top(self):
        if self.head is None :
            return -1 

        return self.head.val 
     

    def isEmpty(self):
        if not self.head:
            return True 
        return False 
